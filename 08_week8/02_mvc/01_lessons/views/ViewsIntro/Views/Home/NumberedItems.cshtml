<h2>Mixed Content Loop:</h2>
@{
    List<string> items = new List<string> { "Laptop", "Monitor", "Keyboard", "Pen", "Notebook", "Eraser", "Marker" };
}
<div class="item-list">
    
        <ol>
            @for(int i = 0; i < items.Count; i++){
                <li>Item: @(i + 1), Name: @items[i]</li>
            }
        </ol>
</div>